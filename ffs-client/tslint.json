{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "extends": ["angular-tslint-rules"],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      [
        "ngx",
        "test",
        "app"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "ngx",
        "test",
        "app"
      ],
      "kebab-case"
    ]
  }
}
